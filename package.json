{
  "name": "react-ssr-webpack-example",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf static/dist && rimraf dist && npm run build:server && npm run build:client",
    "build:client": "webpack --colors --display-error-details --config webpack/prod.config.js",
    "build:server": "webpack --colors --display-error-details --config webpack/server.config.js",
    "dev": "NODE_ENV=development rimraf public/dist && rimraf dist && node bin/dev.js",
    "format": "prettier  --write \"src/**/*.js\"",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint \"src/**/*.{css,scss}\"",
    "lint:css-fix": "stylelint --fix \"src/**/*.{css,scss}\"",
    "lint:fix": "npm run lint:css-fix && npm run lint:js-fix",
    "lint:js": "eslint \"src/**/*.js\"",
    "lint:js-fix": "eslint --fix \"src/**/*.js\"",
    "start": "NODE_ENV=production node bin/prod.js",
    "test": "NODE_ENV=test jest",
    "test:dev": "NODE_ENV=test jest --watch"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "config": "^3.1.0",
    "core-js": "^3.1.3",
    "express": "^4.17.1",
    "helmet": "^3.18.0",
    "morgan": "^1.9.1",
    "query-string": "^6.7.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet-async": "^1.0.2",
    "react-redux": "^7.0.3",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.2",
    "serialize-javascript": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/react": "^8.0.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^3.0.1",
    "ignore-loader": "^0.1.2",
    "jest": "^24.8.0",
    "json-loader": "^0.5.7",
    "memory-fs": "^0.4.1",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.8.6",
    "require-from-string": "^2.0.2",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-scss": "^3.8.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.32.2",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.4",
